{
  "expo": {
    "name": "Paperbull",
    "slug": "paperbull",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-w.jpg",
    "scheme": "paperbull",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.paperbull.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-t.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-w.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-t.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      "expo-background-task",
      [
        "./plugins/expo-foreground-actions"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fdd3cd5b-1001-4695-b16d-d91006d55e22"
      }
    },
    "owner": "shiveshnavin"
  }
}